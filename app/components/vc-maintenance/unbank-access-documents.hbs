{{#if @results.access_documents}}
  <p>
    {{pluralize @results.access_documents.length "item"}} un-banked.
  </p>

  <UiTable>
    <thead>
    <tr>
      <th>RAD</th>
      <th>Type</th>
      <th>Status</th>
      <th>Person</th>
      <th>Status</th>
    </tr>
    </thead>
    <tbody>
    {{#each @results.access_documents as |ad|}}
      <tr>
        <td>
          <PersonLink @personId={{ad.person.id}} @page="tickets-provisions" @callsign="RAD-{{ad.id}}" />
        </td>
        <td>{{this.typeLabel ad.type}}</td>
        <td>{{ad.status}}</td>
        <td>
          <PersonLink @person={{ad.person}} />
        </td>
        <td>{{ad.person.status}}</td>
      </tr>
    {{/each}}
    </tbody>
  </UiTable>
{{else}}
  <p class="text-danger">
    <b>No access documents were found that need to be un-banked.</b>
  </p>
{{/if}}
