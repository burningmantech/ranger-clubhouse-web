<form id={{@formId}}
      class={{@formClass}}
      autocomplete={{this.autocomplete}}
  {{on "submit" this.submitEvent}}
>
  {{yield (hash
            model=this.model
            checkbox=(component "ch-form/checkbox-field" form=this)
            checkboxGroup=(component "ch-form/checkbox-group-field" form=this)
            datetime=(component "ch-form/datetime-field" form=this)
            editor=(component "ch-form/editor-field" form=this)
            number=(component "ch-form/number-field" form=this)
            password=(component "ch-form/password-field" form=this)
            radioGroup=(component "ch-form/radio-group-field" form=this)
            search=(component "ch-form/search-field" form=this)
            select=(component "ch-form/select-field" form=this)
            submit=(component "ch-form/submit" formSubmitAction=this.submitForm)
            text=(component "ch-form/text-field" form=this)
            textarea=(component "ch-form/textarea-field" form=this)
          )
  }}
</form>
