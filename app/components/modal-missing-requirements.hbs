<ModalDialog @title="Sign Up Requirements Not Met" @isInline={{@isInline}} as |Modal|>
  <Modal.body>
    <ScheduleRequirements @requirements={{this.data.requirements}}
                          @person={{this.data.person}}
                          @hasTrainingBlocker={{this.hasTrainingBlocker}} />
  </Modal.body>
  <Modal.footer>
    <UiCloseButton @onClick={{@onClose}} />
  </Modal.footer>
</ModalDialog>
