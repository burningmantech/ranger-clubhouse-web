<ModalDialog @title={{@dialog.title}} @isInline={{@isInline}} as |modal|>
  <modal.body>
    {{this.htmlMessage}}
  </modal.body>
  <modal.footer>
    <UiButtonRow>
      <UiButton @onClick={{@onConfirm}} @type="warning">Confirm</UiButton>
      <UiCancelButton @onClick={{@onClose}} />
    </UiButtonRow>
  </modal.footer>
</ModalDialog>
