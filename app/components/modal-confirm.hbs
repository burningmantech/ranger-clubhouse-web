<ModalDialog @title={{@dialog.title}} @isInline={{@isInline}} as |modal|>
  <modal.body>
    {{this.htmlMessage}}
  </modal.body>
  <modal.footer>
    <UiButtonRow>
      <UiButton @onClick={{@onConfirm}} @type="warning" @size="md">Confirm</UiButton>
      <UiCancelButton @onClick={{@onClose}} @size="md" />
    </UiButtonRow>
  </modal.footer>
</ModalDialog>
