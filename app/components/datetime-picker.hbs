  <EmberFlatpickr
    id={{@id}}
    name={{@name}}
    size={{@size}}
    class={{@classNames}}
    placeholder={{@placeholder}}
    maxlength={{@maxlength}}
    disabled={{@disabled}}
    @date={{or @value ""}}
    @dateFormat={{this.dateFormat}}
    @time_24hr={{true}}
    @locale="en"
    @enableTime={{this.enableTime}}
    @onChange={{null}}
    @onValueUpdate={{this.onChange}}
    @allowInput={{true}}
    @disableMobile={{true}}
    @minDate={{@minDate}}
    @maxDate={{@maxDate}}
    {{did-insert this.elementInserted}}
  />
