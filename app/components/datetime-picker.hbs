<input type="text"
       id={{@id}}
       name={{@name}}
       size={{@size}}
       class={{@classNames}}
       placeholder={{@placeholder}}
       maxlength={{@maxlength}}
       disabled={{@disabled}}
       value={{this.value}}
  {{did-insert this.elementInserted}}
  {{on "change" this.changeEvent}}
  {{on "blur" this.blurEvent}}
/>
