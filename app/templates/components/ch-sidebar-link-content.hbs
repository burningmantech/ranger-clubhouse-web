{{#if iconUrl}}
  <span class="mr-2"><img src="{{iconUrl}}" alt="icon" class="sidebar-icon"></span>
{{else}}
  <span class="fa{{iconType}} fa-{{icon}} fa-fw mr-2"></span>
{{/if}}
<span class="menu-collapsed">{{title}}{{#if badgeText}}
    <span class="badge badge-pill badge-primary ml-1">{{badgeText}}</span>
    {{/if}}
</span>
