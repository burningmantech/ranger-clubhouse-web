<ScheduleManage @person={{this.person}}
                @year={{this.year}}
                @includeLog={{true}} as |onYearChange|
>
  <YearSelect @title="Schedule / Sign Up"
              @year={{this.year}}
              @years={{this.person.years_seen}}
              @subheader={{true}}
              @onChange={{fn this.onYearChange onYearChange}} />
</ScheduleManage>
