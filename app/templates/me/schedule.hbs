<ScheduleManage @person={{this.session.user}}
                @year={{this.year}} as |onYearChange|>
  <YearSelect @title="Schedule & Shift Sign Ups"
              @year={{this.year}}
              @years={{this.person.years_seen}}
              @subheader={{true}}
              @onChange={{fn this.onYearChange onYearChange}} />
  <BackToHome/>
</ScheduleManage>

