<SurveyReport @surveyId={{this.surveyId}} @reportId={{this.reportId}} as |survey|>
  <h1 id="top">{{survey.title}} (Year {{survey.year}})</h1>
  <p>
    <LinkTo @route="training.survey.index" @query={{hash year=survey.year}} @model={{this.training.id}}>
      Back to {{survey.year}} Surveys
    </LinkTo>
  </p>
</SurveyReport>
